// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "tslint.configFile": "tslint.json",
    "typescript.tsdk": "/usr/local/lib/node_modules/typescript/lib",
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "emmet.syntaxProfiles": {
      "vue-html": "html",
      "vue": "html"
    }
}
